function asciiMathModifier(math_input){return math_input.replace(/\*\*/g,"^").replace(/pi/g,Math.PI);}
function computeYScale(width,height,xScale){var xDiff=xScale[1]-xScale[0];var yDiff=height*xDiff/width;return[-yDiff/2,yDiff/2];}
function responsiveDrawGraph(data,width,height,xScale,type){var f_math=asciiMathModifier(data.ascii_math_input);var f_deri=asciiMathModifier(data.ascii_math_diff);if(type=='diffsteps'){try{functionPlot({width:width,height:height,xDomain:xScale,yDomain:computeYScale(width,height,xScale),target:'#deriPlotting',grid:true,data:[{fn:f_math,sampler:'builtIn',graphType:'polyline',color:'#05b378',derivative:{fn:f_deri,updateOnMouseMove:true}},{fn:f_deri,sampler:'builtIn',graphType:'polyline'}]});$('#deriPlottingQuote').html('<img class="bhtd-deri-loading" src="/static/mathimgs/bhtd-graph-deri-quote.png">')}
catch(e){$('#diffgraph-29smx02ls').html('<div class="center">Oop! Hệ thống chưa thể vẽ đồ thị cho bài toán này.</div>')}}
else if(type=='intsteps'){try{functionPlot({width:width,height:height,xDomain:xScale,yDomain:computeYScale(width,height,xScale),target:'#deriPlotting',grid:true,data:[{fn:f_math,sampler:'builtIn',graphType:'polyline',color:'#05b378'},{fn:f_deri,sampler:'builtIn',graphType:'polyline'}]});$('#integPlottingQuote').html('<img class="bhtd-deri-loading" src="/static/mathimgs/bhtd-graph-integ-quote.png">')}
catch(e){$('#diffgraph-29smx02ls').html('<div class="center">Oop! Hệ thống chưa thể vẽ đồ thị cho bài toán này.</div>')}}}
function requestStep(variable,expression,type){$('#diffstep-x8fd7s2y').replaceWith('<img class="center bhtd-deri-loading" id="diffstep-1l6pnv82" src="/static/mathimgs/math-loading.svg">');var expression_c=expression.replace(/\s/g,'');$.ajax({url:'/local/math/'+type+'/',type:'POST',dataType:'json',data:{variable:variable,expression:expression_c},success:function(data){if(data.status=='OK'){$('#diffstep-1l6pnv82').remove();$('.showDiffStep').html(data.output);MathJax.Hub.Queue(["Typeset",MathJax.Hub,'showDiffStep']);$('#diffgraph-1e82x6wk').remove();var div_width=$('#diffgraph-29smx02ls').width();var div_height=~~(div_width/1.65);var xScale=[-6,6];responsiveDrawGraph(data,div_width,div_height,xScale,type);$(window).on('resize',function(){var div_width=$('#diffgraph-29smx02ls').width();var div_height=~~(div_width/1.65);var xScale=[-6,6];responsiveDrawGraph(data,div_width,div_height,xScale,type);});}
else{$('#diffstep-1l6pnv82').remove();$('.showDiffStep').html('<div class="center">Oop! Hệ thống chưa thể lập các bước giải cho bài toán này, điều này sẽ được cải thiện trong thời gian tới.</div>');}},error:function(jXHR,textStatus,errorThrown){$('#diffstep-1l6pnv82').remove();$('.showDiffStep').html('<div class="center">Oop! Đã có lỗi xảy ra, vui lòng thử lại sau.</div>');}});}
