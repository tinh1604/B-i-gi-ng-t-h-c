if(searchform.length>0){searchform.focus();searchform.select2({placeholder:"Tìm kiếm nhanh: ghi từ hoặc câu bạn muốn tìm kiếm...",minimumInputLength:2,delay:200,width:'100%',ajax:{url:TITLE_SEARCH_URL,dataType:'json',data:function(term,page){return{q:term};},processResults:function(data,page){return{results:data.items};}},templateResult:title_format,escapeMarkup:function(m){return m;}})
searchform.on("change",function(e){var url=e.currentTarget['value']
window.location=url})}}
function select2Input(queryStr){var input=$(queryStr)
var $select=$('<select class="'+input.attr('class')+'" multiple="multiple"></select>')
if(input.val()!=""){input.val().split(',').forEach(function(item){$select.append('<option value="'+item+'" selected="selected">'+item+'</option>')});}
$select.insertAfter(input)
input.hide()
$select.change(function(){if($select.val()!=null){input.val($select.val().join(","));}
else{input.val('');}});return $select;}
