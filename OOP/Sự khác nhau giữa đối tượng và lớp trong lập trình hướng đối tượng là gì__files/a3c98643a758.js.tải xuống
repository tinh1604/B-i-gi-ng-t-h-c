function title_format(row){if(typeof(row.url)==='undefined'||typeof(row.text)==='undefined'||typeof(row.context)==='undefined'||typeof(row.author)==='author'){link='<span>Đang tìm kiếm...</span>';}
else{link='<a href="'+row.url+'"/>'+row.text+'</a><div class="in">'+row.context+' bởi <b>'+row.author+'</b></div>';}
return $(link)}
