$(".bhtdTypingNote").delay(1000).fadeOut();}
else if(message_data.text!=''&&message_data.istyping!='BHTD_USER_TYPING_2@3_0!8'){$(".bhtdTypingNote").remove();$("div.chat div.conversation").append('<div class="message">'+((message_data.sender==extra_para.user_id)?('<div class="bhtd-outcome-message"><div class="bhtd-outcome-message-pullright"><div class="bhtd-sent"><div class="bhtd-sent-msg"><p>'+message_data.text.replace(/&/g,'&amp;').replace(/</g,'&lt;').replace(/>/g,'&gt;').replace(/\n/g,'<br />')+'</p></div></div> <div class="bhtd-outcome-message-img"><a href="/u/'+extra_para.user_id+'/">'+extra_para.user_img_url.replace(/&lt;/g,'<').replace(/&quot;/g,'"').replace(/&amp;/g,'&').replace(/&gt;/g,'>')+'</a></div></div></div>'):('<div class="bhtd-income-message"><div class="bhtd-income-message-img"><a href="/u/'+extra_para.partner_id+'/">'+extra_para.partner_img_url.replace(/&lt;/g,'<').replace(/&quot;/g,'"').replace(/&amp;/g,'&').replace(/&gt;/g,'>')+'</a></div> <div class="bhtd-received"><div class="bhtd-received-msg"><p>'+message_data.text.replace(/&/g,'&amp;').replace(/</g,'&lt;').replace(/>/g,'&gt;').replace(/\n/g,'<br />')+'</p></div></div></div>'))+'</div>');}
scroll_chat_window();}
ws.onclose=function(){setTimeout(function(){start_chat_ws()},5000);};}
if("WebSocket"in window){if(pre_ws!=''){pre_ws.onclose=function(){};pre_ws.close();}
start_chat_ws();pre_ws=ws;}else{return false;}
function send_message(){var textarea=$("textarea#message_textarea");if($.trim(textarea.val())==""){return false;}
if(ws.readyState!=WebSocket.OPEN){return false;}
ws.send(textarea.val());textarea.val("");}
$("form.message_form div.send i").click(send_message);$("textarea#message_textarea").keydown(function(e){if(e.ctrlKey&&e.which==13){var textVal=$("textarea#message_textarea").val();textVal+='\n'
$("textarea#message_textarea").val(textVal);}
if(e.which==13&&!e.ctrlKey){e.preventDefault();send_message();}});var lastTypedTime=new Date(0);function refreshTypingStatus(){var textArea=$("textarea#message_textarea");if(textArea.length>0){var typingDelayMillis=5000;if((textArea.val()!='')&&(new Date().getTime()-lastTypedTime.getTime()<typingDelayMillis)){ws.send('BHTD_USER_TYPING_2@3_0!8');}}}
