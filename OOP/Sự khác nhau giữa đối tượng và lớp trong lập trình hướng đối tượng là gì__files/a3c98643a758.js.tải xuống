function wordDefViewMore(word_name,lex,word_index,word_order){$.ajax({url:'/local/dict/more/',type:"POST",dataType:'json',data:{word_name:word_name,lex:lex,word_index:word_index},success:function(data){if(data.status=="OK"){var data_obj=data.content;var word_hyponyms=data_obj.hyponyms;var hyponyms_html=''
if(word_hyponyms.length>0){hyponyms_html='<p>Từ cụ thể:</p>';for(const word of word_hyponyms){Object.values(word)[0]?hyponyms_html+='<a href="/tu-dien/en-vi/'+Object.keys(word)[0]+'/">'+Object.keys(word)[0].replace(/_/g,' ')+'</a>, ':hyponyms_html+=' '+Object.keys(word)[0]+', '}}
var word_hypernyms=data_obj.hypernyms;var hypernyms_html='';if(word_hypernyms.length>0){hypernyms_html='<p>Từ tổng quát:</p>';for(const word of word_hypernyms){Object.values(word)[0]?hypernyms_html+='<a href="/tu-dien/en-vi/'+Object.keys(word)[0]+'/">'+Object.keys(word)[0].replace(/_/g,' ')+'</a>, ':hypernyms_html+=' '+Object.keys(word)[0]+', '}}
var word_meronyms=data_obj.meronyms;var meronyms_html='';if(word_meronyms.length>0){meronyms_html='<p>Từ bộ phận:</p>';for(const word of word_meronyms){Object.values(word)[0]?meronyms_html+='<a href="/tu-dien/en-vi/'+Object.keys(word)[0]+'/">'+Object.keys(word)[0].replace(/_/g,' ')+'</a>, ':meronyms_html+=' '+Object.keys(word)[0]+', '}}
var result=hyponyms_html+hypernyms_html+meronyms_html;$('#bhtddict_more_'+word_order).html(result);$('#bhtddict_more_'+word_order).addClass('bhtddict-more-active');}
else{$('#bhtddict_more_'+word_order).html('<div>Oop! Không tìm thấy.</div>');}},error:function(jXHR,textStatus,errorThrown){$('#bhtddict_more_'+word_order).html('<div>Oop! Đã có lỗi xảy ra, vui lòng thử lại.</div>');}});}
