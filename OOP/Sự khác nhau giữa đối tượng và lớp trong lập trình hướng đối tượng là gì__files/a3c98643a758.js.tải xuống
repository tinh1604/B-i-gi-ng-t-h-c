else{$('.bhtdWriteNewMsgNote').html('<span id="bhtdWriteNewMsgNoteChild" class="text-center alert alert-info">'+data.status+'</span>');}},error:function(jXHR,textStatus,errorThrown){$('.bhtdWriteNewMsgNote').html('<span id="bhtdWriteNewMsgNoteChild" class="text-center alert alert-danger">Oop! Không thể gửi tin nhắn của bạn</span>');}});}
else{return false;}}
