<script>
    function single_tap(){
      if($('.mini-viec').hasClass('show_mini_viec')){
          $('.mini-viec').removeClass('show_mini_viec');
          $('#bg_sw').hide();
      }else{
          $('.mini-viec').addClass('show_mini_viec');
          $('#bg_sw').show();
      }

    }

    function double_tap(){
        window.location.href = '/';
    }

    var tapped=false
    $(".show_box .mobile_doubtap").on("touchstart",function(e){
        if(!tapped){
          tapped=setTimeout(function(){
              single_tap()
              tapped=null
          },300); //wait 300ms
        } else {
          clearTimeout(tapped);
          tapped=null
          double_tap()
        }
        e.preventDefault()
    });
</script>
<script>
  function getCookie(cname) {
    var name = cname + "=";
    var decodedCookie = decodeURIComponent(document.cookie);
    var ca = decodedCookie.split(';');
    for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}

function checkCookie() {
    var userid = getCookie("UID");
    if (userid != "") {
      sessionStorage.setItem("UID", getCookie('UID'));
    } else {
      sessionStorage.clear();
    }
}
checkCookie();

